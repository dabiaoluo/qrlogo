#!/bin/sh

# ============================================================
# qr_bench_relase: run qrlogo benchmarks
# ============================================================
# Prerequisites:
# Run qr_init
# ============================================================

set -o nounset
set -o errexit

PGM="$(basename $0)"
cd "$(dirname "$0")/.."


# ============================================================

. cfg/upstream.sh


# ============================================================

printf "\n>>> >>> %s\n" "benchmarking"
cargo +"${RUST_TOOLCHAIN}" bench "$@"
printf "\n>>> >>> %s\n" "done"
